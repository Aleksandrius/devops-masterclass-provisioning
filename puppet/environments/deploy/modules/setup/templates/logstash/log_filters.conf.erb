
filter {
  if [type] == "syslog" {
    grok {
      match => [ "message", "%{SYSLOGBASE2}" ]
    }
    date {
      match => ["timestamp", "MMM  d HH:mm:ss", "MMM dd HH:mm:ss"]
    }
  }
}


filter {
  if [type] == "jenkins-server" {
    mutate {
      add_field => ["@message_type", "jenkins"]
      add_field => ["@message", "%{message}"]
    }
    multiline {
      pattern => "^%{MONTH} %{MONTHDAY}, %{YEAR} %{TIME} (AM|PM)"
      negate => true
      what => "previous"
    }
  }
}

